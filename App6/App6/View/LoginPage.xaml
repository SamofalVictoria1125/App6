<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App6.View.LoginPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="Login" Placeholder="Введите логин"/>
            <Entry x:Name="Password" Placeholder="Введите пароль"/>
            <Button Text="Войти" Clicked="Button_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>